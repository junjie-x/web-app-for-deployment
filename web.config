<?xml version="1.0" encoding="UTF-8"?>

<configuration>

<system.webServer>

<handlers>

<add name="httpPlatformHandler" path="*" verb="*"

modules="httpPlatformHandler" resourceType="Unspecified" />

</handlers>

<httpPlatform processPath="/usr/lib/jvm/zulu-8-azure-amd64/bin/java.exe"

arguments="-Djava.net.preferIPv4Stack=true -Dport.http=%HTTP_PLATFORM_PORT% -jar &quot;%HOME%\site\wwwroot\bin\eventhubs-binder-sample-1.1.0-SNAPSHOT.jar&quot;"

stdoutLogEnabled="true"

startupRetryCount='10'>

</httpPlatform>
cd bin
</system.webServer>

</configuration>